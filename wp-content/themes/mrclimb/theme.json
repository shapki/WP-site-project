					"name": "Dark to Element vertical"
				},
				{
					"slug": "mrclimb-dark-to-element-150",
					"gradient": "linear-gradient(150deg, var(--wp--preset--color--mrclimb-dark), var(--wp--preset--color--mrclimb-element))",
					"name": "Dark to Element diagonal"
				},
				{
					"slug": "mrclimb-element-to-decoration-180",
					"gradient": "linear-gradient(180deg, var(--wp--preset--color--mrclimb-element), var(--wp--preset--color--mrclimb-decoration))",
					"name": "Element to Decoration vertical"
				},
				{
					"slug": "mrclimb-element-to-decoration-150",
					"gradient": "linear-gradient(150deg, var(--wp--preset--color--mrclimb-element), var(--wp--preset--color--mrclimb-decoration))",
					"name": "Element to Decoration diagonal"
				},
				{
					"slug": "mrclimb-decoration-to-light-180",
					"gradient": "linear-gradient(180deg, var(--wp--preset--color--mrclimb-decoration), var(--wp--preset--color--mrclimb-light))",
					"name": "Decoration to Light vertical"
				},
				{
					"slug": "mrclimb-decoration-to-light-150",
					"gradient": "linear-gradient(150deg, var(--wp--preset--color--mrclimb-decoration), var(--wp--preset--color--mrclimb-light))",
					"name": "Decoration to Light diagonal"
				},
				{
					"slug": "mrclimb-decoration-60-to-light-180",
					"gradient": "linear-gradient(180deg, var(--wp--preset--color--mrclimb-decoration) 60%, var(--wp--preset--color--mrclimb-light) 40%)",
					"name": "Decoration 60 to Light 40 vertical"
				},
				{
					"slug": "mrclimb-decoration-60-to-light-150",
					"gradient": "linear-gradient(150deg, var(--wp--preset--color--mrclimb-decoration) 60%, var(--wp--preset--color--mrclimb-light) 40%)",
					"name": "Decoration 60 to Light 40 diagonal"
				},
				{
					"slug": "mrclimb-dark-to-light-180",
					"gradient": "linear-gradient(180deg, var(--wp--preset--color--mrclimb-dark) 70%, var(--wp--preset--color--mrclimb-light) 30%)",
					"name": "Dark to Light vertical"
				},
				{
					"slug": "mrclimb-dark-to-light-150",
					"gradient": "linear-gradient(150deg, var(--wp--preset--color--mrclimb-dark) 70%, var(--wp--preset--color--mrclimb-light) 30%)",
					"name": "Dark to Light diagonal"
				},
				{
					"slug": "mrclimb-dark-to-background-180",
					"gradient": "linear-gradient(180deg, var(--wp--preset--color--mrclimb-dark) 70%, var(--wp--preset--color--mrclimb-background) 30%)",
					"name": "Dark to Background vertical"
				},
				{
					"slug": "mrclimb-dark-to-background-150",
					"gradient": "linear-gradient(150deg, var(--wp--preset--color--mrclimb-dark) 70%, var(--wp--preset--color--mrclimb-background) 30%)",
					"name": "Dark to Background diagonal"
				},
				{
					"slug": "mrclimb-decoration-to-background-180",
					"gradient": "linear-gradient(180deg, var(--wp--preset--color--mrclimb-decoration) 70%, var(--wp--preset--color--mrclimb-background) 30%)",
					"name": "Decoration to Background vertical"
				},
				{
					"slug": "mrclimb-decoration-to-background-150",
					"gradient": "linear-gradient(150deg, var(--wp--preset--color--mrclimb-decoration) 70%, var(--wp--preset--color--mrclimb-background) 30%)",
					"name": "Decoration to Background diagonal"
				},
				{
					"slug": "mrclimb-decoration-to-element-180",
					"gradient": "linear-gradient(180deg, var(--wp--preset--color--mrclimb-decoration) 70%, var(--wp--preset--color--mrclimb-element) 30%)",
					"name": "Decoration to Element vertical"
				},
				{
					"slug": "mrclimb-decoration-to-element-150",
					"gradient": "linear-gradient(150deg, var(--wp--preset--color--mrclimb-decoration) 70%, var(--wp--preset--color--mrclimb-element) 30%)",
					"name": "Decoration to Element diagonal"
				},
				{
					"slug": "mrclimb-decoration-to-element-115",
					"gradient": "linear-gradient(115deg, var(--wp--preset--color--mrclimb-decoration) 45%, rgba(0,0,0,0) 45%)",
					"name": "Decoration to Element vertical"
				}
			]
