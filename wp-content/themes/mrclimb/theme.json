		"color": {
			"palette": [
				{
					"color": "#FFFFFF",
					"name": "Background",
					"slug": "mrclimb-background"
				},
				{
					"color": "#000000",
					"name": "Text",
					"slug": "mrclimb-text"
				},
				{
					"color": "#1E2121",
					"name": "Dark",
					"slug": "mrclimb-dark"
				},
				{
					"color": "#FC8428",
					"name": "Decoration",
					"slug": "mrclimb-decoration"
				},
				{
					"color": "#FAF4E5",
					"name": "Element",
					"slug": "mrclimb-element"
				},
				{
					"color": "#F9F7F7",
					"name": "Light",
					"slug": "mrclimb-light"
				}
			],
			"gradients": [
				{
					"slug": "mrclimb-dark-to-element-180",
