		"custom": {
			"typography": {
				"font-size": {
					"heading-1": "clamp(2rem, 1.75rem + 1.25vw, 3.25rem)",
					"heading-2": "clamp(1.65rem, 1.48rem + 0.85vw, 2.5rem)",
					"heading-3": "clamp(1.4rem, 1.28rem + 0.6vw, 2rem)",
					"heading-4": "clamp(1.25rem, 1.18rem + 0.35vw, 1.6rem)",
					"heading-5": "clamp(1rem, 0.95rem + 0.25vw, 1.25rem)",
					"heading-6": "clamp(1rem, 0.95rem + 0.25vw, 1.25rem)"
				}
			},
			"line-height": {
				"tiny": 1.15,
				"small": 1.25,
				"medium": 1.4,
				"normal": 1.6
			},
			"color": {
				"footer": "var(--wp--preset--color--mrclimb-light)",
				"sidebar": "var(--wp--preset--color--mrclimb-light)",
				"footer-text": "var(--wp--preset--color--mrclimb-text)",
				"element-text": "var(--wp--preset--color--mrclimb-text)",
				"hero-text": "var(--wp--preset--color--mrclimb-light)"
			},
			"drop-cap": {
