					"font-family": "var(--wp--preset--font-family--manrope)",
					"font-size": "clamp(6rem, 5.5rem + 1.25vw, 7rem)",
					"font-weight": "600",
					"font-style": "normal",
					"line-height": "var(--wp--custom--line-height--normal)"
				},
				"spacing": {
					"margin": "0.05em 0.1em 0 0",
					"paddig": "0"
				}
			}
		},
		"spacing": {
			"padding": true,
			"margin": true,
			"blockGap": true,
			"units": ["%", "px", "em", "rem", "vh", "vw"]
		},
